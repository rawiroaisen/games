<!doctype html>
<html>
<head>
</head>
<body>
    <h1><%= game.name %></h1>
    <img src="<%= game.thumb_url %>" alt="<%= game.name %>">
    <object type="application/x-shockwave-flash" data="<%= game.swf_url %>" width="<%= game.screen.width %>" height="<%= game.screen.height %>">
        <% Object.keys(game.flashvars).forEach(function(name){ %>
            <% if(name != 'toObject'){ %>
                <param name="<%= name %>" value="<%= game.flashvars[name] %>">
            <% } %>
        <% }) %>
    </object>
</body>
</html>